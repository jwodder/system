[apache-auth]
enabled = true

[apache-badbots]
enabled = true

[apache-fakegooglebot]
enabled = true

[apache-nohome]
enabled = true

[apache-noscript]
enabled = true
maxretry = 3

[apache-notaproxy]
enabled  = true
port     = http,https
logpath  = %(apache_access_log)s
maxretry = 1

[apache-overflows]
enabled = true

[apache-shellshock]
enabled = true

